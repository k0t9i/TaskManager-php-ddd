<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="TaskManager\Projects\Domain\Entity\Request" table="requests">
        <id name="id" type="request_id" length="36" />
        <field name="userId" column="user_id" type="project_user_id" length="36" />
        <field name="status" type="request_status"  />
        <field name="changeDate" column="change_date" type="request_change_date" />
    </entity>
</doctrine-mapping>